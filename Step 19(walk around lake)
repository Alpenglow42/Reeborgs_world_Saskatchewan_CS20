from library import turn_right, turn_around, find_north


def stay_dry():
    while not object_here():
        if right_is_clear():
            turn_right()
            if front_is_clear():
                move()
            
            
        elif front_is_clear():
            move()
            
        elif not front_is_clear():
            turn_left()

            
think(1)            
put()
if front_is_clear():
    move()
stay_dry()
