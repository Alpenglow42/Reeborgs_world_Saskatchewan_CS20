

from library import turn_right, turn_around, find_north


def hurdle_1():
    while not at_goal():
        move()
        if wall_in_front():
            turn_left()
            move()
            turn_right()
            if front_is_clear():
                move()
                turn_right()
                move()
                turn_left()
            elif not fron_is_clear():
                turn_left()
                while wall_on_right():
                    move()

                    
hurdle_1()
